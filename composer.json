{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "predis/predis": "1.0.*",
        "codex/core": "1.0.*",
        "codex/composer-plugin": "1.0.*",
        "codex/addon-phpdoc": "1.0.*",
        "codex/addon-git": "1.0.*",
        "sebwite/git": "dev-master as 1.0.x-dev"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "plesk php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "plesk php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "plesk php artisan optimize",
            "plesk php artisan vendor:publish --tag=public --force"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "plesk php artisan vendor:publish --tag=public --force"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
